{
  "manifest_version": 2,

  "name": "Spotify Player",
  "description": "This extension will be a player for a desktop Spotify client.",
  "version": "1.2",

  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "connect.html"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [ {
    "js": [ "js/bootstrap.min.js", "js/jquery-3.5.1.min.js", "js/connect.js", "js/player.js"],
    "matches": [ "http://*/*", "https://*/*"]
}]
}
